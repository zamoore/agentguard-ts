{
  "name": "@zamoore/agentguard-ts",
  "version": "0.1.0",
  "description": "A Node.js package for AgentGuard",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "generate:version": "tsx scripts/generate-version.ts",
    "prebuild": "pnpm generate:version",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:performance": "vitest tests/performance --run",
    "test:cli": "vitest tests/cli.test.ts",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.test.json",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test && pnpm run lint:check",
    "prerelease": "pnpm generate:version",
    "release": "changeset publish",
    "changeset": "changeset"
  },
  "keywords": [],
  "author": "zamoore <zack.moore89@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/your-repo/issues"
  },
  "homepage": "https://github.com/your-username/your-repo#readme",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
